{
  "production": {
    "app_function": "wsgi_app.app",
    "apigateway_description": "GraphQL Auto Generation API",
    "api_key_required": true,
    "aws_region": "ap-south-1",
    "project_name": "py-flask-graphql-autogen",
    "runtime": "python3.6",
    "s3_bucket": "py-flask-graphql-autogen",
    "environment_variables": {
      "env": "prod"
    },
    "extra_permissions": [
      {
        "Effect": "Allow",
        "Action": [
          "secretsmanager:*"
        ],
        "Resource": "*"
      }
    ],
    "memory_size": 1024
  }
}